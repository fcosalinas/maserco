<h1>Listing materials</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Description</th>
    <th>Unit</th>
    <th>Loss</th>
    <th>Adqvalue</th>
    <th>Transport</th>
    <th>Unitarycost</th>
    <th>Sitevalue</th>
    <th>Realcost</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @materials.each do |material| %>
  <tr>
    <td><%= material.code %></td>
    <td><%= material.description %></td>
    <td><%= material.unit %></td>
    <td><%= material.loss %></td>
    <td><%= material.adqvalue %></td>
    <td><%= material.transport %></td>
    <td><%= material.unitarycost %></td>
    <td><%= material.sitevalue %></td>
    <td><%= material.realcost %></td>
    <td><%= link_to 'Show', material %></td>
    <td><%= link_to 'Edit', edit_material_path(material) %></td>
    <td><%= link_to 'Destroy', material, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Material', new_material_path %>
