<h1>Listing proposes</h1>

<table>
  <tr>
    <th>Obra</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @proposes.each do |propose| %>
  <tr>
    <td><%= propose.obra %></td>
    <td><%= link_to 'Show', propose %></td>
    <td><%= link_to 'Edit', edit_propose_path(propose) %></td>
    <td><%= link_to 'Destroy', propose, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

